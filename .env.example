# Copy to .env and fill values (DO NOT commit real .env)
BOT_TOKEN=
WEBAPP_BASE_URL=http://127.0.0.1:8000/webapp
# If true, /auth/init will return a JWT signed with JWT_SECRET.
# If false, backend will validate initData but won't issue tokens (stateless mode).
USE_JWT=false
JWT_SECRET=replace-with-strong-secret-if-using-jwt